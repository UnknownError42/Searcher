	# Расширения для сканирования (.php5, .inc ...)
	extensions:
	- php
	# Заголовок для отчета
	title: Мой отчет по разработке моего модуля
	# Генерировать доки чтобы показать устаревшие методы
	deprecated: true
	# Генерировать доки для меток @todo
	todo: true
	# Генерировать ссылку на загрузку документации
	download: true
	# Путь к файлу , проекту или директории для которого необходимо собрать документацию
	source/
	# Конечная директория (здесь можно посмотреть результат, относительный путь от директории запуска apigen)
	destination: ./api
