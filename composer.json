{
    "name": "stanislav-web/phalcon-searcher",
    "description": "Phalcon Database Searcher",
    "type": "library",
    "status": "maintained",
    "keywords" : [
        "phalcon",
        "phalcon search",
        "phalcon searcher"
    ],
    "support": {
        "issues": "https://github.com/stanislav-web/Searcher/issues"
    },
    "minimum-stability": "dev",
    "homepage" : "https://github.com/stanislav-web/Searcher",
    "license" : "MIT",
    "authors" : [{
         "name": "Stanislav WEB",
         "email": "stanisov@gmail.com",
         "homepage": "http://reds.com",
         "role": "Developer"
    }],

    "require" : {
         "php" : ">=5.3"
    },

    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master"
    },

	"autoload": {
		"classmap": ["src/"],
        "psr-4": {
            "Test\\Searcher\\": "src/Searcher"
        }
	},

	"autoload-dev": {
		"psr-4": {
            "Searcher\\Searcher\\Tests\\": "phpunit/src/Searcher"
		}
	},

    "extra": {
        "branch-alias": {
            "dev-master": "1.0*"
        }
    }
}
